{
  "name": "aes-ccm",
  "version": "1.0.4",
  "description": "AES CCM support using OpenSSL",
  "main": "lib/index.js",
  "scripts": {
    "configure": "node-gyp configure",
    "build": "node-gyp build",
    "pretest": "semistandard",
    "coverage": "nyc npm test",
    "coveragehtml": "nyc report -r html",
    "precoveragehtml": "npm run coverage",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "test": "ava test",
    "preinstall": "true",
    "install": "node-pre-gyp install --fallback-to-build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erdtman/aes-ccm.git"
  },
  "keywords": [
    "aes",
    "ccm",
    "openssl"
  ],
  "author": "Samuel Erdtman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erdtman/aes-ccm/issues"
  },
  "homepage": "https://github.com/erdtman/aes-ccm",
  "dependencies": {
    "node-gyp-install": "^2.0.1",
    "node-pre-gyp": "^0.12.0"
  },
  "devDependencies": {
    "ava": "*",
    "node-pre-gyp-github": "^1.4.3",
    "npm-run-all": "*",
    "nyc": "*",
    "semistandard": "*"
  },
  "binary": {
    "module_name": "aes_ccm",
    "module_path": "./build/Release",
    "host": "https://github.com/erdtman/aes-ccm/releases/download/",
    "remote_path": "{version}"
  }
}
